{
  "metadata": [
    {
      "src": [
        {
            "src": "../",
            "files": [
              "DrawBug.csproj"
            ]
        }
      ],
      "dest": "api",
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "DrawBug",
      "_appFooter": "DrawBug Documentation",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/**/*"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": true
  }
}